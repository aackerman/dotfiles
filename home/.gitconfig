[alias]
  ci = commit
  co = checkout
  psuh = push
  phus = push
  puhs = push
  pus  = push
  psh  = push
  phs  = push
  pul  = pull
  yolo = push --force
  back = reset --soft HEAD^
  undo = reset --soft HEAD^
  amend = commit --amend -C HEAD
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[color]
  ui = true
  branch = true
  diff = true
[push]
  default = current
[core]
  excludesfile = ~/.gitignore
[user]
  name = Aaron Ackerman
  email = theron17@gmail.com
[help]
  autocorrect = true
